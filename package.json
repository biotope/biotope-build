{
  "name": "@biotope/build",
  "version": "7.0.0-8",
  "description": "An advanced development and build runtime to create frontent ecosystems",
  "repository": {
    "type": "git",
    "url": "https://github.com/biotope/biotope-build.git"
  },
  "author": "Biotope",
  "license": "MIT",
  "main": "dist/index.js",
  "scripts": {
    "start": "npm run clean && npm run copy:preview && tsc --watch",
    "build": "npm run clean && npm run copy:preview && tsc",
    "copy:preview": "cpy src/devPreview dist/devPreview",
    "clean": "rimraf dist",
    "release": "np --no-yarn",
    "test": "ts-mocha -p tsconfig.json src/**/*.spec.ts",
    "eslint:lint": "eslint --ignore-path .gitignore .",
    "eslint:fix": "eslint --fix --ignore-path .gitignore ."
  },
  "pre-commit": "eslint:lint",
  "dependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-transform-classes": "7.4.4",
    "@babel/preset-env": "7.4.5",
    "autoprefixer": "^9.6.1",
    "console-emoji": "0.0.2",
    "cssnano": "^4.1.10",
    "del": "4.1.1",
    "eslint": "6.0.1",
    "gulp-connect": "^5.7.0",
    "gulp-postcss": "^8.0.0",
    "gulp-sass": "^4.0.2",
    "jshint": "2.10.2",
    "jshint-stylish": "2.2.1",
    "node-sass": "^4.12.0",
    "opn": "5.3.0",
    "prettier": "1.18.2",
    "prettier-eslint": "9.0.0",
    "rimraf": "^2.6.3",
    "ts-node": "^8.3.0",
    "typescript": "2.9.2"
  },
  "resolutions": {
    "webpack-stream/webpack": "4.34.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/chai-fs": "^2.0.1",
    "@types/mocha": "^5.2.7",
    "chai": "^4.2.0",
    "chai-fs": "^2.0.0",
    "cpy-cli": "^2.0.0",
    "gulp": "^4.0.2",
    "gulp-filelist": "^2.0.1",
    "np": "~5.0.3",
    "ts-mocha": "^6.0.0"
  }
}
